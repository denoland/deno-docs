{
  "nodeModulesDir": true,
  "imports": {
    "@b-fuze/deno-dom": "jsr:@b-fuze/deno-dom@^0.1.47",
    "@std/async": "jsr:@std/async@^0.224.2",
    "@std/fs": "jsr:@std/fs@^0.229.3",
    "@std/media-types": "jsr:@std/media-types@^0.224.1",
    "@std/path": "jsr:@std/path@^0.225.1",
    "@types/node": "npm:@types/node@20.12.7",
    "browserslist": "npm:browserslist@4.23.0",
    "dax": "jsr:@david/dax@^0.40.1",
    "lightningcss": "npm:lightningcss@1.15",
    "ts-morph": "npm:ts-morph@22.0.0"
  },
  "tasks": {
    "types:deno": "deno run --allow-read --allow-write --allow-run --allow-env --allow-sys deno-docs.ts",
    "types:node": "deno run --allow-read --allow-write=. --allow-env --allow-sys node-docs.ts",
    "types": "deno task types:deno && deno task types:node",
    "doc:deno": "DENO_INTERNAL_HTML_DOCS=deno deno doc --html --name=Deno --output=gen/deno types/deno.d.ts",
    "doc:web": "DENO_INTERNAL_HTML_DOCS=deno deno doc --html --name=Web --output=gen/web types/web.d.ts",
    "doc:node": "DENO_INTERNAL_HTML_DOCS=node deno doc --html --name=Node --output=gen/node types/node/[!_]*",
    "doc": "deno task doc:deno && deno task doc:web && deno task doc:node",
    
    "serve": "deno run --allow-read=. --allow-net --watch serve.ts",
    "build": "deno run --allow-read=.,../build --allow-write=. --allow-env build.ts"
  },
  "exports": {
    "./assert": "./types/node/node__assert.d.ts",
    "./assert/strict": "./types/node/node__assert--strict.d.ts",
    "./async_hooks": "./types/node/node__async_hooks.d.ts",
    "./buffer": "./types/node/node__buffer.d.ts",
    "./child_process": "./types/node/node__child_process.d.ts",
    "./cluster": "./types/node/node__cluster.d.ts",
    "./console": "./types/node/node__console.d.ts",
    "./constants": "./types/node/node__constants.d.ts",
    "./crypto": "./types/node/node__crypto.d.ts",
    "./dgram": "./types/node/node__dgram.d.ts",
    "./diagnostics_channel": "./types/node/node__diagnostics_channel.d.ts",
    "./dns": "./types/node/node__dns.d.ts",
    "./dns/promises": "./types/node/node__dns--promises.d.ts",
    "./domain": "./types/node/node__domain.d.ts",
    "./events": "./types/node/node__events.d.ts",
    "./fs": "./types/node/node__fs.d.ts",
    "./fs/promises": "./types/node/node__fs--promises.d.ts",
    "./http": "./types/node/node__http.d.ts",
    "./http2": "./types/node/node__http2.d.ts",
    "./https": "./types/node/node__https.d.ts",
    "./inspector": "./types/node/node__inspector.d.ts",
    "./module": "./types/node/node__module.d.ts",
    "./net": "./types/node/node__net.d.ts",
    "./os": "./types/node/node__os.d.ts",
    "./path": "./types/node/node__path.d.ts",
    "./perf_hooks": "./types/node/node__perf_hooks.d.ts",
    "./process": "./types/node/node__process.d.ts",
    "./punycode": "./types/node/node__punycode.d.ts",
    "./querystring": "./types/node/node__querystring.d.ts",
    "./readline": "./types/node/node__readline.d.ts",
    "./readline/promises": "./types/node/node__readline--promises.d.ts",
    "./repl": "./types/node/node__repl.d.ts",
    "./sea": "./types/node/node__sea.d.ts",
    "./stream": "./types/node/node__stream.d.ts",
    "./stream/consumers": "./types/node/node__stream--consumers.d.ts",
    "./stream/promises": "./types/node/node__stream--promises.d.ts",
    "./stream/web": "./types/node/node__stream--web.d.ts",
    "./string_decoder": "./types/node/node__string_decoder.d.ts",
    "./test": "./types/node/node__test.d.ts",
    "./test/reporters": "./types/node/node__test--reporters.d.ts",
    "./timers": "./types/node/node__timers.d.ts",
    "./timers/promises": "./types/node/node__timers--promises.d.ts",
    "./tls": "./types/node/node__tls.d.ts",
    "./trace_events": "./types/node/node__trace_events.d.ts",
    "./tty": "./types/node/node__tty.d.ts",
    "./url": "./types/node/node__url.d.ts",
    "./util": "./types/node/node__util.d.ts",
    "./util/types": "./types/node/node__util--types.d.ts",
    "./v8": "./types/node/node__v8.d.ts",
    "./vm": "./types/node/node__vm.d.ts",
    "./wasi": "./types/node/node__wasi.d.ts",
    "./worker_threads": "./types/node/node__worker_threads.d.ts",
    "./zlib": "./types/node/node__zlib.d.ts"
  },
  "exclude": [
    "types",
    "gen"
  ]
}
